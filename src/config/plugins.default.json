{
  "system-monitor": {
    "enabled": true,
    "settings": {
      "autoStart": false,
      "interval": 5000,
      "logToFile": false,
      "alertThresholds": {
        "cpuUsage": 80,
        "memoryUsage": 90
      }
    }
  },
  "discord-integration": {
    "enabled": false,
    "settings": {
      "autoConnect": false,
      "defaultChannel": null,
      "notificationPreferences": {
        "systemAlerts": false,
        "performanceWarnings": false
      }
    }
  },
  "spotify-control": {
    "enabled": false,
    "settings": {
      "autoAuthenticate": false,
      "defaultPlaylist": null
    }
  },
  "obs-advanced": {
    "enabled": true,
    "settings": {
      "autoConnect": false,
      "address": "localhost:4444",
      "password": "",
      "autoDetectScenes": {
        "enabled": false,
        "interval": 30000
      },
      "streamingPresets": {
        "defaultScene": null,
        "automaticTransitions": false
      },
      "notificationSettings": {
        "streamStarted": true,
        "streamStopped": true,
        "sceneChanged": false,
        "performanceWarnings": {
          "droppedFrames": 10,
          "lowFPS": 30
        }
      }
    }
  }
}
